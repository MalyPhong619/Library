<h1>Library</h1>

<h2>Books - <a href="/books/add">Add Book</a> </h2>
<ul>
  <% @books.each do |book| %>
    <li><a href="books/<%= book.id %>"><%= book.title %></a></li>
  <% end %>
</ul>


<h2>Patrons - <a href="/patrons/add">Add Patron</a> </h2>

<ul>
  <% @patrons.each do |patron| %>
    <li><a href="patrons/<%= patron.id %>"><%= patron.name %></a></li>
  <% end %>
</ul>
